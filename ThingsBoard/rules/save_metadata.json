        },
        "type": "org.thingsboard.rule.engine.transform.TbTransformMsgNode",
        "name": "Set Metadata",
        "debugMode": false,
        "configuration": {
          "jsScript": "metadata.entity_id = msg.id.id;\nmetadata.entity_name = msg.name;\nmetadata.entity_action = msgType;\n\nreturn {msg: msg, metadata: metadata, msgType: msgType};"
        }
      },
      {
        "additionalInfo": {
          "layoutX": 552,
          "layoutY": 150
        },
        "type": "org.thingsboard.rule.engine.filter.TbJsFilterNode",
        "name": "Filter for Workers",
        "debugMode": false,
        "configuration": {
          "jsScript": "if (msgType !== 'ENTITY_CREATED' && msgType !== 'ENTITY_UPDATED' && msgType !== 'ENTITY_DELETED') {\n    return false;\n}\n\nif (msg.id.entityType !== 'ASSET') {\n    return false;\n}\n\nif (msg.type !== 'WORKER' && msgType !== 'ENTITY_UPDATED') {\n    return false;\n}\n\nreturn true;"
        }
      },
      {
        "additionalInfo": {
          "layoutX": 1375,
          "layoutY": 149
        },
        "type": "org.thingsboard.rule.engine.metadata.TbGetAttributesNode",
        "name": "Get Attributes",
        "debugMode": false,
        "configuration": {
          "clientAttributeNames": [],
          "sharedAttributeNames": [],
          "serverAttributeNames": [
            "workers"
          ],
          "latestTsKeyNames": []
        }
      },
      {
        "additionalInfo": {
          "layoutX": 1924,
          "layoutY": 151
        },
        "type": "org.thingsboard.rule.engine.transform.TbTransformMsgNode",
        "name": "Set Message",
        "debugMode": false,
        "configuration": {
          "jsScript": "var workers = JSON.parse(metadata.ss_workers||'{}');\n\nif (metadata.entity_action === 'ENTITY_CREATED') {\n    workers[metadata.entity_id] = {\n        name: metadata.entity_name\n    };\n} \nelse if (metadata.entity_action === 'ENTITY_UPDATED') {\n    if (msg.type === 'WORKER') {\n        workers[metadata.entity_id] = {\n            name: metadata.entity_name\n        };\n    }\n    else {\n        delete workers[metadata.entity_id];\n    }\n} \nelse if (metadata.entity_action === 'ENTITY_DELETED') {\n    delete workers[metadata.entity_id];\n} \n\nmsg = { \n    workers: JSON.stringify(workers) \n};\n\nmsgType = 'POST_ATTRIBUTES_REQUEST';\n\nreturn {msg: msg, metadata: metadata, msgType: msgType};"
        }
      },
      {
        "additionalInfo": {
          "layoutX": 2198,
          "layoutY": 149
        },
        "type": "org.thingsboard.rule.engine.telemetry.TbMsgAttributesNode",
        "name": "Save Attributes",
        "debugMode": false,
        "configuration": {
          "scope": "SERVER_SCOPE"
        }
      },
      {
        "additionalInfo": {
          "layoutX": 275,
          "layoutY": 150
        },
        "type": "org.thingsboard.rule.engine.filter.TbJsSwitchNode",
        "name": "Switch Metadata Type",
        "debugMode": false,
        "configuration": {
          "jsScript": "return ['Workers'];"
        }
      },
      {
        "additionalInfo": {
          "layoutX": 1651,
          "layoutY": 151
        },
        "type": "org.thingsboard.rule.engine.filter.TbJsFilterNode",
        "name": "Check Asset Type Changed",
        "debugMode": false,
        "configuration": {
          "jsScript": "if (msg.type !== 'WORKER') {\n    var workers = JSON.parse(metadata.ss_workers||'{}');\n    \n    if (!workers[metadata.entity_id]) {\n        return false;\n    }\n}\n\nreturn true;"
        }
      }
