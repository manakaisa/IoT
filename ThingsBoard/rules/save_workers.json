        "configuration": {
          "jsScript": "metadata.targetDeviceName = (metadata.ss_worker_prefix||'') + msg.worker_id;\nmetadata.worker_id = msg.worker_id;\n\nreturn {msg: msg, metadata: metadata, msgType: msgType};"
        }
      },
      {
        "additionalInfo": {
          "layoutX": 550,
          "layoutY": 151
