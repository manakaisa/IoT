          "entityCacheExpiration": 300,
          "createEntityIfNotExists": false,
          "changeOriginatorToRelatedEntity": true,
          "removeCurrentRelations": false
        }
      },
      {
        "additionalInfo": {
          "layoutX": 153,
          "layoutY": 301
        },
        "type": "org.thingsboard.rule.engine.transform.TbTransformMsgNode",
        "name": "Set Metadata",
        "debugMode": false,
        "configuration": {
          "jsScript": "metadata = {\n    targetDeviceName: 'Metadata',\n    entity_id: msg.id.id,\n    entity_name: msg.name,\n    entity_action: msgType\n}\n\nreturn {msg: msg, metadata: metadata, msgType: msgType};"
        }
